@model IList<ProductOverviewModel>

@using Nop.Web.Models.Catalog;
@if (Model.Count > 0)
{
    <div class="also-purchased-products-grid product-grid">
        <div class="title">
            <h2>@T("Products.AlsoPurchased")</h2>
        </div>
        <ul class="item-grid">
            @foreach (var item in Model)
            {
                <li class="item-box">@Html.Partial("_ProductBox", item)</li>
            }
        </ul>
    </div>
}